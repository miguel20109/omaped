	<parameter name="dni" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
 a.dni
,CONVERT(concat(a.apaterno,' ',a.amaterno,' ',a.nombres) USING utf8) as nombres
,DATE_FORMAT(a.fecha_nacimiento, "%d/%m/%Y") as fecha_nacimiento
,c.descripcion as sexo
,a.estado_civil
,d.descripcion as tipo_doc
,a.direccion
,b.telefono
,a.edad
,CASE WHEN b.espadre = '1' THEN 'SI' ELSE 'NO' END as espadre
,CASE WHEN b.certificado = '1' THEN 'SI' ELSE 'NO' END as certificado
,CASE WHEN b.carnet_conadis = '1' THEN 'SI' ELSE 'NO' END as carnet_conadis
,DATE_FORMAT(b.carnet_vigencia, "%d/%m/%Y") as carnet_vigencia
,b.institucion_otorgo
,b.numerohijos
,b.contacto
,b.telefono as telefono2
,a1.Descripcion as descri_vivienda
,a2.Descripcion as descri_viven
,a3.Descripcion as descri_construccion
,a4.Descripcion as descri_servicios
,a5.Descripcion as descri_instruccion
,a6.Descripcion as descri_estudios
,a7.Descripcion as descri_seguro
,b.apoderado
,b.oficio
,b.noestudia
,b.limita_capacidad
,b.gustaria_capacitarse
,b.cuantos_viven
,b1.region
,b2.provincia
,b3.distrito
,a.referencia
,CONVERT(concat(b4.nombre,' ',b4.apellido) USING utf8) as created_user
,DATE_FORMAT(b.created_at, "%d/%m/%Y") as created_at
FROM personas a
INNER JOIN personas_discapacidad b on a.dni=b.dni
INNER JOIN sexo c on a.idsexo=c.id
INNER JOIN tipo_documento d on a.IDtipo_documento=d.id
left JOIN contenedor_omaped a1 on b.idvivienda=a1.id
left JOIN contenedor_omaped a2 on b.idviven=a2.id
left JOIN contenedor_omaped a3 on b.idconstruccion=a3.id
left JOIN contenedor_omaped a4 on b.idservicios=a4.id
left JOIN contenedor_omaped a5 on b.idinstruccion=a5.id
left JOIN contenedor_omaped a6 on b.idestudios=a6.id
left JOIN contenedor_omaped a7 on b.seguro=a7.id
left JOIN ubigeo_region b1 on a.region=b1.id
left JOIN ubigeo_provincia b2 on a.provincia=b2.id
left JOIN ubigeo_distrito b3 on a.distrito=b3.id
left JOIN usuarios b4 on b.created_user=b4.id
where a.dni=$P{dni}
;]]>
